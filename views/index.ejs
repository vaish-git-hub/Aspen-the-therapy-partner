                            <div class="section-title text-start">
                                <h1 class="display-5 mb-4">Feedback</h1>
                            </div>

                            <form method="post" action="/" id="feedback-message">
                                <div class="row g-3">
                                    <div class="col-12 col-sm-6">
                                        <input type="text" class="form-control border-0" placeholder="Your Name"
                                            name="name" style="height: 55px;">
                                    </div>
                                    <div class="col-12 col-sm-6">
                                        <input type="email" class="form-control border-0" placeholder="Your Email"
                                            name="email" style="height: 55px;">
                                    </div>
                                    <div class="col-12 col-sm-6">
                                        <input type="text" class="form-control border-0" placeholder="Your Mobile"
                                            name="mobile" style="height: 55px;">
                                    </div>
                                    <div class="col-12 col-sm-6">
                                        <select class="form-select border-0" style="height: 55px;" name="gender">
                                            <option selected>Gender</option>
                                            <option value="female">Female</option>
                                            <option value="male">Male</option>
                                            <option value="others">Others</option>
                                        </select>
                                    </div>
                                    <div class="col-12">
                                        <textarea class="form-control border-0" placeholder="Special Note"
                                            name="note"></textarea>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary w-100 py-3" type="submit">Submit</button>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div id="feedback-message">
                                        <% if (typeof success !=='undefined' && success.length> 0) { %>
                                            <div class="alert alert-success">
                                                <%= success[0] %>
                                            </div>
                                            <% } %>
                                                <% if (typeof error !=='undefined' && error.length> 0) { %>
                                                    <div class="alert alert-danger">
                                                        <%= error[0] %>
                                                    </div>
                                                    <% } %>
                                    </div>

                                </div>

                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Feedback End -->

        <script>
            $(document).ready(() => {
                $("#feedback-message").on('submit', (event) => {
                    event.preventDefault();
                    $.ajax({
                        type: 'POST',
                        url: '/admin', // Update the URL to match your backend route
                        data: $('#feedback-message').serialize(),
                        dataType: "json",
                        success: (response) => {
                            $('#feedback-message')[0].reset();
                            var successDiv = document.getElementById("feedback-message");
                            var successMessage = successDiv.querySelector(".alert.alert-success");

                            if (successMessage) {
                                // If a success message already exists, update its content
                                successMessage.innerHTML = response.success;
                            } else {
                                // If no success message exists, create and append it
                                successMessage = document.createElement("div");
                                successMessage.className = "alert alert-success";
                                successMessage.innerHTML = response.success;
                                successDiv.appendChild(successMessage);
                            }

                            // Clear the message after 3 seconds
                            setTimeout(() => {
                                successMessage.innerHTML = "";
                            }, 3000);
                        },
                        error: () => {
                            document.getElementById("feedback-message").innerHTML = "Feedback submission failed";
                        }
                    });
                });
            });
        </script>


